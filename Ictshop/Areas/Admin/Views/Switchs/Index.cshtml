@model IEnumerable<Ictshop.Models.Switch>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Quản lý Switch</h2>

<p>
    @Html.ActionLink("Thêm mới hệ điều hành", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã Switch
        </th>
        <th>
            Tên Switch
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Maswitch)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tenswitch)
    </td>
    <td>
        @Html.ActionLink("Sửa", "Edit", new { id = item.Maswitch }) |
        @Html.ActionLink("Chi tiết", "Details", new { id = item.Maswitch }) |
        @Html.ActionLink("Xóa ", "Delete", new { id = item.Maswitch })
    </td>
</tr>
}

</table>
